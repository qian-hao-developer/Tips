select _id, original_network_id as networkId, display_number, display_name, hex(internal_provider_data) as data, (internal_provider_flag1&255) as branch, ((internal_provider_flag1>>8)&255) as preset from channels;
select * from OrmaBranch where inputId = "jp.co.sei.streamcruiser.tvinput2/.services.TbInputService";
select _id, type, service_type as serviceType, original_network_id as networkId, service_id as serviceId, transport_stream_id as tsId, display_number as displayNum, display_name as displayNam from channels;
